/* Base =================================================== */

// GRID		 			=====================================

// Susy Grid Variables
$total-columns  : 7;
$column-width   : 4em;
$gutter-width   : 1em;
$grid-padding   : $gutter-width;
$container-style: fluid;

// ROOT 				=====================================

@-webkit-viewport { 
	width : device-width; 
}
@-moz-viewport { 
	width : device-width; 
}
@-ms-viewport { 
	width : device-width; 
}
@-o-viewport { 
	width : device-width; 
}
@viewport { 
	width : device-width; 
}

html {
	background : $bodycolor;
	font-size : 62.5%;
	-webkit-overflow-scrolling : touch;
	-webkit-tap-highlight-color : lighten($basecolor, 75%);
	-webkit-text-size-adjust : 100%;
	-ms-text-size-adjust : 100%;
}

body {
	@include font-size();

	background-color : transparent;
	font-family : $basefont;
	line-height : $baselineheight;
	color : $textcolor;
	margin : 0;
	padding : 0;
	width : 100%;
}


// TYPOGRAPHY ====================================================

#{headings()} {
	margin : 0;
	font-family : $baseheadingfont;
	font-weight : $baseheadingfontweight;
	color : $baseheadingfontcolor;
	text-rendering : optimizelegibility; 
}

h1 small,
h2 small {
	font-weight : normal;
	color : $lightgray; 
}

h1,
.h1 {
	margin-bottom : $baselineheight / 2;
	@include font-size(32);
	line-height : 1.2; 
}

h2,
.h2 {
	margin-bottom : $baselineheight / 2;
	@include font-size(24);
	line-height : 1.2; 
}

h3,
.h3 {
	margin-bottom : $baselineheight / 3;
	@include font-size(48);
	line-height : 1; 
}

h4,
.h4 {
	margin-bottom : $baselineheight;
	@include font-size(18);
	line-height : 1.25; 
}

h5,
.h5 {
	@include font-size(16);
	margin-bottom : $baselineheight; 
}

h6,
.h6 {
	margin-bottom : $baselineheight;
	@include font-size(14);
	font-weight : normal;
	letter-spacing : 1px;
	text-transform : uppercase; 
}

p.lead {
	@include font-size(24);
	line-height : $baselineheight; 
}

p,
ol,
ul,
dl,
address {
	margin-bottom : $baselineheight;
	@include font-size(16);
	line-height : $baselineheight; 
}

p {
	line-height: 2em;
}

small {
	@include font-size(14); 
}

ul,
ol {
	margin : 0 0 $baselineheight 0;
	padding : 0;

	li {
		list-style: none;
	}
}

li ul,
li ol {
	@include font-size(16); 
}

// blockquote

blockquote  {
	margin : 0 0 $baselineheight 0;
	padding-left : $gutter-width;
	font-style : normal; 
	text-align: center;
}

q {
	quotes : none; 
}

blockquote:before,
blockquote:after,
q:before,
q:after {
	content : '';
	content : none; 
}

cite {
	font-style : normal; 
}

// definition lists

dl,
dd {
	margin-bottom : $baselineheight;  
}

dt {
	font-weight : bold; 
}

.dl-horizontal {

dt {
	float : left;
	clear : left;
	width : 20.25%;
	text-align : right; 
}

dd {
	margin-left : 22.78%; }
}

// text-level

abbr[title] {
	border-bottom : 1px dotted $bordercolor;
	cursor : help; 
}

b, strong {
	font-weight : bold; 
}

dfn {
	font-style : italic; 
}

ins {
	background-color : lighten($basecolor, 75%);
	color : $textcolor;
	text-decoration : none; 
}

mark {
	background-color : lighten($basecolor, 75%);
	color : $textcolor;
	font-style : italic;
	font-weight : bold; 
}

pre,
code,
kbd,
samp {
	font-family : $codefont;
	@include font-size(14);
	line-height : $baselineheight; 
}

pre {
	white-space : pre;
	white-space : pre-wrap;
	word-wrap : break-word; 
}

sub,
sup {
	position : relative;
	@include font-size(12);
	line-height : 0;
	vertical-align : baselineheight; 
}

sup {
	top : -0.5em; 
}

sub {
	bottom : -0.25em; 
}


// GENERAL	 			=====================================

.nav {
	display: none;
}

.header {
	@include container;

	color: $garthgray;
	font-family: $altfont;
	height: 132px;
	padding-top: 24px;
	padding-bottom: 20px;
	position: relative;
	text-align: left;
	text-transform: uppercase;

	&:after {
		@include at2x("bump_white", 139px, 21px);
		background-repeat: no-repeat;
		content: "";
		height: 21px;
		position: absolute;
		bottom: -21px;
		left: 50%;
		margin-left: -71px;
		width: 139px;
		z-index: 1;
	}

	h1 {
		@include font-size(50);
		font-weight: normal;
		line-height: .9;
		margin-bottom: 8px;

		span {
			@include font-size(22);
			display: block;
		}
		a {
			color: $basecolor;

			&:hover {
				color: lighten($garthgray, 10%);
				text-decoration: none;
			}
		}
	}
	p {
		@include font-size(16);
		margin: 0;
	}

	.social {
		position: absolute;
		top: 10px;
		right: 2%;
		margin: 16px 0 0 0;

		li { margin-bottom: 4px; }

		a {
			i {
				color: $garthgray;
				margin-bottom: 10px;
			
				&:hover {
					color: lighten($garthgray, 10%);
					text-decoration: none;
				}
			}
		}

		.tw {
			@include font-size(15);
		}
		.fb {
			@include font-size(17);
		}
		.li {
			@include font-size(17);
		}
	}
}

.panel {
	position: relative;
	width: 100%;

	.label {
		border-right: 1px solid $white;
		border-bottom: 1px solid $white;
		height: 34px;
		padding: 0 8px;
		position: absolute;
		top: 0;
		left: 0;

		h2 {
			@include font-size(22);
			color: $white;
			font-family: $altfont;
			font-weight: normal;
			line-height: 1.9;
			text-transform: uppercase;
		}
	}

	.story {
		@include container;

		padding-bottom: 60px;
		padding-top: 60px;
		max-width: 90%;
	}
}


// SECTIONS 			=====================================

#garth {
	background: url(../img/garth.jpg) 50% 0 no-repeat fixed;
	height: 600px;
	// background-size: 300% auto;
	// background-attachment: scroll;

	.story {
		padding-top: 100px;
	}

	.copy {
		@include span-columns(4,7);
		line-height : $baselineheight; 
	}
}


#what {
	background: #42ba90;

	.story {
		margin: 0 auto;
	}

	.graphic {
		margin: 0 auto;
		text-align: center;
	}

	hr { margin: 20px 0; }

	.copy {

		&.left {

			p {
				@include font-size(22);
				line-height : $baselineheight; 
			}
		}

		&.right {

			p {
				@include font-size(16);
			}
		}
	}
}


#who {
	background: #1e4393;

	.graphic {
		text-align: center;
		margin-bottom: 20px;
	}

	p {
		@include font-size(16);

		&.lead {
			@include font-size(31);
			margin-bottom: 1em;
		}
	}
}


#work {
	background: #349946;

	h3 {
		font-family: $altfont;
		text-transform: uppercase;
		text-align: center;
	}

	hr.dotted {
		display: none;
	}

	ul {
		margin: 0;
		padding: 0;

		li {
			border-top: 2px dotted $white;
			padding: 16px 0;

			p {
				@include font-size(19);
				line-height: 1em;
				margin-bottom: 0;

				&.title {
					position: relative;
					margin-bottom: .2em;

					&:before {
						@include cssTriangle(6px, $white, left);
						position: absolute;
						left: -12px;
						top: 5px;
					}
				}
			}
		}
	}
}


#together {
	background: #f26621;

	blockquote {
		max-width: 90%;
		margin: 0 auto 2em;

		br {
			display: none;
		}

		span {
			@include font-size(29);
			display: inline-block;
			line-height: $baselineheight;
			margin-bottom: 20px;
			margin-bottom: 1em;

			&.lead {
				@include font-size(60);
				margin-bottom: .2em;
			}
		}
	}

	ul {
		padding: 0 2%;
		margin-bottom: 3em;

		li {
			border-bottom: 2px dotted $white;
			@include font-size(19);
			padding: 16px 0;

			&:first-child {
				border-top: 2px dotted $white;
			}
		}
	}

	p.outro {
		clear: both;
		@include font-size(29);
		line-height: $baselineheight;
		margin: 0 auto $baselineheight;
		max-width: 80%;
		text-align: center;
	}
}


#contact {
	background: #323232;

	.copy {
		h3	{
			@include font-size(47);
			font-family: $altfont;
			margin-bottom: .2em;
			text-transform: uppercase;
		}
		p {
			@include font-size(22);
			line-height: $baselineheight;
		}
	}

	.footer {
		@include container;
		padding: 1em 0 3em 0;
		border-top: 1px solid #fffef4;
		clear: both;
		margin: 0 auto;
		width: 90%;

		.col {
			font-family: "Helvetica Neue", arial, helvetica;
			color: #fffef4;
			@include font-size(11);
			line-height: $baselineheight;
			margin-bottom: .5em;
		}
	}
